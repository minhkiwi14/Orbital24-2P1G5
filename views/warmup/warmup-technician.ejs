  <body>
    <header class="header-technician">
      <a
        href="/technician-management"
        class="technician-management-home"
        id="home"
        >Management Page</a
      >
      <a
        class="quit score"
        href="/technician-score-management"
        >Score Management</a
      >
      <a class="quit" href="/">Quit Game</a>
    </header>

    <main>
      <h1>Round 1: Warm-up</h1>
      <section class="question-score-timer warmup">
        <p class="question-board">Question</p>
        <p class="next-question">Next Question</p>
        <p class="answer-board">Answer</p>
      </section>

      <div class="player-score" id="player-score-warmup-technician">
        <div class="name-and-score">
          <h3>Nhat Minh</h3>
          <p>1000</p>
          <a class="correct">Correct</a>
          <a class="incorrect">Incorrect</a>
          <a class="bonus5">+5</a>
          <a class="bell"><i class="fa-solid fa-bell"></i></a>
        </div>
        <div class="name-and-score">
          <h3>Huy Tung</h3>
          <p>1000</p>
          <a class="correct">Correct</a>
          <a class="incorrect">Incorrect</a>
          <a class="bonus5">+5</a>
          <a class="bell"><i class="fa-solid fa-bell-slash"></i></a>
        </div>
        <div class="name-and-score">
          <h3>Minh Nhat</h3>
          <p>1000</p>
          <a class="correct">Correct</a>
          <a class="incorrect">Incorrect</a>
          <a class="bonus5">+5</a>
          <a class="bell"><i class="fa-solid fa-bell"></i></a>
        </div>
        <div class="name-and-score">
          <h3>Tung Huy</h3>
          <p>1000</p>
          <a class="correct">Correct</a>
          <a class="incorrect">Incorrect</a>
          <a class="bonus5">+5</a>
          <a class="bell"><i class="fa-solid fa-bell"></i></a>
        </div>
      </div>

      <ul class="management-buttons">
        <li>
          <a href="/warmup-technician">Warmup</a>
        </li>
        <li>
          <a href="/obstacle-technician"
            >Obstacle</a
          >
        </li>
        <li>
          <a
            href="/acceleration-technician"
            >Acceleration</a
          >
        </li>
        <li>
          <a href="/finish-technician">Finish</a>
        </li>
      </ul>

